.art-header{
	font-size:0;
}
.art-img{
	width:750rpx;
	height: 422rpx;
}
.tapbar{
	width:100%;
	position:fixed;
	top:0;
	left:0;
	background:#fff;
	height:80rpx;
	border-bottom:2rpx solid #e1e1e1;
	z-index:2;
}
.tapview{
	margin-top:82rpx;
}
.art-title{
	text-align:left;
	padding:20rpx 30rpx 10rpx;
	line-height:32rpx;
	font-size:28rpx;
	color:#222;
	background:#fff;
}
.art-price{
	font-size:32rpx;
	color:#be8914;
	text-align:left;
	padding:8rpx 30rpx 18rpx;
	background:#fff;
}
.art-all{
	position:relative;
}
.art-view{
	position:absolute;
	right:66rpx;
	top:50%;
	font-size:24rpx;
	color:#666;
}
.art-right{
	position:absolute;
	width:28rpx;
	height:28rpx;
	right:30rpx;
	top:50%;
}
.point{
	margin:30rpx;

}
.point .title,.brand .title{
	font-size:28rpx;
	color:#222;
	display: block;
	padding:5rpx 0 15rpx;
}
.point .descrite,.brand .descrite{
	color:#222;
	font-size:24rpx;
	line-height: 34rpx;
	display: block;
}
.brand{
	padding-bottom:100rpx;
	background:#fff;
}
.bantop{
	font-size: 0;
	position:relative;
	width: 750rpx;
	height: 422rpx;
	margin-bottom:100rpx;
}
.bantop .banner{
	width: 750rpx;
	height: 422rpx;
}
.bantop .bg{
	position:absolute;
	top: 0;
	left: 0;
	width: 750rpx;
	height: 422rpx;
	background: rgba(0,0,0,0.2);
}
.bantop .logo{
	position: absolute;
	width: 120rpx;
	height: 120rpx;
	border-radius: 60rpx;
	border: 1px solid rgba(0,0,0,0.1);
	left: 50%;
	margin-left: -60rpx;
	bottom:-60rpx;
}
.viewimg{
	width:100%;
	height: 100%;
	margin:0;
	padding:0;
}
.art-bot{
	position: fixed;
	left:0;
	bottom: 0;
	z-index: 2;
	background: #fff;
	width:100%;
	height:88rpx;
	text-align: center;
}
.cart image{
	width:80rpx;
	height: 64rpx;
	padding: 12rpx 20rpx;
}
.joinbtn{
	font-size: 32rpx;
	background: #be8914;
	color: #fff;
	/*padding: 26rpx 0;*/
	line-height: 88rpx;
}
.gobuy{
	font-size: 32rpx;
	background: #222;
	color: #fff;
	/*padding: 28rpx 0;*/
	line-height: 88rpx;
}
.buycard{
	height:1334rpx;
	width:750rpx;
	background:rgba(0,0,0,0);
	position:fixed;
	bottom:0;
	left:0;
	z-index:4;
	visibility: hidden;
    -webkit-transform: translate3d(0,100%,0);
    transform: translate3d(0,100%,0);
    -webkit-transition: all 0.3s ease 0.05s;
    transition: all 0.3s ease 0.05s;
}
.buycard.active{
	visibility: visible;
    -webkit-transform: translate3d(0,0,0);
    transform: translate3d(0,0,0);
}
.f-top{
	width:750rpx;
	height:454rpx;
	position:absolute;
	top:0;
	left:0;
	background:rgba(0,0,0,0);
}
.fromes{
	position:absolute;
	bottom:0;
	left:0;
	background:#fff;
	width:750rpx;
	height:880rpx;
}
.buybg{
	height:1334rpx;
	width:750rpx;
	background:rgba(0,0,0,0.6);
	position:fixed;
	bottom:0;
	left:0;
	z-index: 3;
}
.f-header{
	padding:30rpx 0 25rpx;
	margin:0 30rpx;
	border-bottom:1px solid rgba(0,0,0,0.1);
	position: relative;
	height:170rpx;
}
.skuimg{
	position: absolute;
	top: 30rpx;
	left: 0;
	width:168rpx;
	height:168rpx;
	border: 2rpx solid rgba(0,0,0,0.1);
}
.f-price{
	vertical-align: top;
	display:block;
	margin-left: 190rpx;
	margin-right: 80rpx;
}
.f-title{
	color: #222;
	font-size: 30rpx;
	line-height: 38rpx;
	overflow : hidden;
    height:74rpx;
    padding-top:6rpx;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    margin-bottom: 6rpx;
}
.fromes .art_close{
	width: 45rpx;
    height: 45rpx;
    position: absolute;
	top:0;
	right:0;
	padding: 20rpx;
}
.fromes .art_close .close{
	width:45rpx;
	height:45rpx;
}
.f-price .price{
	color:#be8914;
	font-size:30rpx;
	display:block;
	padding-bottom:10rpx;
}
.f-price .smesg{
	color:#333;
	font-size:26rpx;
	display:block;
	padding-bottom:6rpx;
}
.sku-list{
	position:relative;
	margin:0 30rpx;
	padding:20rpx 0 20rpx;
	border-bottom:1px solid rgba(0,0,0,0.1);
}
.sku-list .ui.header,.sku-num .ui.header{
	font-size:28rpx;
	color:#666;
}
.sku-num{
	position:relative;
	margin:0 30rpx;
	padding:20rpx 0;
}
.sku-name{
	margin-left:-8rpx;
	margin-top:24rpx;
	max-height: 260rpx;
	overflow: scroll;
}
.sku-name .fiu-btn{
	background:rgba(0,0,0,0);
	margin:8rpx;
	font-size:24rpx;
	display:inline-block;
	color:#666;
	border:1px solid #c1c1c1;
	position : relative ; 
	padding-left : 28rpx ; 
	padding-right : 28rpx ; 
	box-sizing : border-box ;
	text-align : center ; 
	text-decoration : none ; 
	line-height : 2.55555556 ; 
	border-radius : 10rpx ; 
	-webkit-tap-highlight-color : transparent ; 
	overflow : hidden ;
}
.sku-name .fiu-btn.active{
	background:#000;
	color:#fff;
	border:1px solid #000;
}
.numset{
	margin-top:20rpx;
	font-size:0;
}
.numminus{
	display:inline-block;
	font-size:45rpx;
	width:60rpx;
	height:60rpx;
	text-align:center;
	line-height:50rpx;
	border:1px solid rgba(0,0,0,0.1);
	vertical-align: top;
	border-radius:0 !important;
	background:rgba(0,0,0,0);
}
.sku-shu{
	display:inline-block;
	font-size:32rpx;
	width:80rpx;
	height:60rpx;
	line-height:60rpx;
	text-align:center;
	border-top:1px solid rgba(0,0,0,0.1);
	border-bottom:1px solid rgba(0,0,0,0.1);
	vertical-align: top;
	padding:0 12rpx;
}
.numadd{
	display:inline-block;
	font-size:40rpx;
	width:60rpx;
	height:60rpx;
	text-align:center;
	line-height:50rpx;
	border:1px solid rgba(0,0,0,0.1);
	vertical-align: top;
	border-radius:0;
	background:rgba(0,0,0,0);
}
.numminus.active,.numadd.active{
	background:#f1f1f1;
}
.cart{
	position: relative;
}
.cart .text{
	background:#be8914;
	border-radius:50%;
	font-size:20rpx;
	width:40rpx;
	height:40rpx;
	text-align:center;
	position:absolute;
	top:10rpx;
	left:50%;
	line-height:40rpx;
	color:#fff;
}
.fiu-btn.nobtn{
	background: #f1f1f1 !important;
	color: #222 !important;
	border:1px solid #c1c1c1 !important;
}